                    "${workspaceFolder}/Drivers/**",
                    "${workspaceFolder}/Core/Src/**",
                    "${workspaceFolder}/Common/**",
                    "${workspaceFolder}/Common/Middlewares/**",
                    "${workspaceFolder}/Common/Streaming/**",
                    "${workspaceFolder}/Common/Middlewares/ST/STM32_USB_Device_Library/**"
