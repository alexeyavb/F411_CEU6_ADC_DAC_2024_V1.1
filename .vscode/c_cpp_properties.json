{
    "configurations": [
        {
            "name": "STM32",
            "compilerPath": "/opt/ST/STM32CubeCLT/GNU-tools-for-STM32/bin/arm-none-eabi-gcc",
            "includePath": [
                "${workspaceFolder}/Core/Inc", 
                "${workspaceFolder}/Drivers/STM32F4xx_HAL_Driver/Inc", 
                "${workspaceFolder}/Drivers/STM32F4xx_HAL_Driver/Inc/Legacy", 
                "${workspaceFolder}/Drivers/CMSIS/Include", 
                "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32F4xx/Include", 
                "${workspaceFolder}/Drivers/BSP/STM32F411/", 
                "${workspaceFolder}/Drivers/HAL_USB_Library", 
                "${workspaceFolder}/USB_Devcie/AUD_Streaming10/Inc", 
                "${workspaceFolder}/Common/Streaming/Inc", 
                "${workspaceFolder}/Common/Middlewares/ST/STM32_USB_Device_Library/Class/AUDIO_10/Inc", 
                "${workspaceFolder}/Common/Middlewares/ST/STM32_USB_Device_Library/Core/Inc", 
                "${workspaceFolder}/Drivers/HAL_USB_Library",
            ],
            "defines": [
                "STM32F411xE",
                "USE_HAL_DRIVER",
                "USE_MATH_FF",
                "USE_USB_FS",
                "USE_USB_AUDIO_CLASS_10",
